{
  "name": "myproject",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "post-build": "cp -r ./src/views ./dist",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && npm run post-build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/mongodb": "^6.0.1",
    "@fastify/multipart": "^7.1.0",
    "@fastify/schedule": "^3.0.0",
    "@fastify/sensible": "^4.1.0",
    "@fastify/static": "^6.5.0",
    "@fastify/websocket": "^7.0.0",
    "ajv": "^8.11.0",
    "ajv-keywords": "^5.1.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.30.1",
    "dotenv": "^16.0.1",
    "fastify": "^4.0.0",
    "fastify-api-key": "^1.0.2",
    "fastify-bcrypt": "^1.0.0",
    "fastify-cli": "^4.4.0",
    "fastify-file-upload": "^4.0.0",
    "fastify-mailer": "^2.3.1",
    "fastify-mongodb": "^4.2.0",
    "fastify-multer": "^2.0.3",
    "fastify-multipart": "^5.4.0",
    "fastify-plugin": "^3.0.1",
    "fastify-socket.io": "^4.0.0",
    "formidable": "^2.0.1",
    "mongoose": "^6.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.7",
    "nodemon": "^2.0.19",
    "socket.io": "^4.5.1",
    "toad-scheduler": "^1.6.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.5",
    "@types/ws": "^8.5.3",
    "concurrently": "^7.0.0",
    "fastify-tsconfig": "^1.0.1",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
